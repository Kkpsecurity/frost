# n8n Configuration for FROST Application
# Copy this to .env.n8n and update the values

# Basic Authentication
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_me_please

# Encryption Key (Generate a secure random string)
# Use: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
N8N_ENCRYPTION_KEY=

# Database Password (if using PostgreSQL)
N8N_DB_PASSWORD=n8n_secure_password

# Timezone
GENERIC_TIMEZONE=America/New_York

# Logging Level (debug, info, warn, error)
N8N_LOG_LEVEL=info
